{
  "name": "Modify Sdarot",
  "version": "1.0",
  "description": "Change Control extension",
  "manifest_version": 2,
  "permissions": ["activeTab", "<all_urls>"],  

    "browser_action": {          
        "default_popup": "popup.html"  
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js":      ["content.js"]
  }]
}